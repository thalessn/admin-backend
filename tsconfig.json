{
  "compilerOptions": {
    "incremental": true,                  
    "target": "es2017",                      
    "module": "commonjs",                    
    "baseUrl": "./src",                                  
    "esModuleInterop": true,                 
    "forceConsistentCasingInFileNames": true,
    "strict": true,                          
    "noImplicitAny": false,                
    "strictNullChecks": false,             
    "skipLibCheck": true,
    "paths": {
      "#seedwork/*": [
        "./@seedwork/*"
      ],
      "#category/*": [
        "./category/*"
      ]
    },
    "declaration": true,
    "outDir": "./dist"          
  },
  "include": ["src/"]
}
